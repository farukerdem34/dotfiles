# Completion Sources
if  command -v kubectl &> /dev/null;
then 
eval "$(kubectl completion bash)"
fi

if  command -v hcloud &> /dev/null;
then 
eval "$(hcloud completion bash)"
fi

if command -v fzf &> /dev/null;
then
eval "$(fzf --bash)"
fi

if command -v docker &> /dev/null;
then
eval "$(docker completion bash)"
fi
